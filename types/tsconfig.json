{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "baseUrl": ".",
    "paths": {
      "@ricado/api-client": ["."]
    },
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  }
}
