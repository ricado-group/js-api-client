{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-memberof-namespace"
    ],
    "templates": {
        "cleverLinks": true
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md",
        "template": "node_modules/ink-docstrap/template"
    }
}