{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-memberof-namespace"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md",
        "template": "node_modules/tidy-jsdoc",
        "prism-theme": "prism-custom"
    },
    "metadata": {
        "title": "RICADO Gen 4 API JS Client",
        "logo": "https://ricado-gen3web-static.s3-ap-southeast-2.amazonaws.com/ricado_logo.svg"
    },
    "styles": {
        "primary-color": "#0db47e",
        "secondary-color": "#1b3752",
        "nav-bg-color": "#1b3752",
        "nav-heading-color": "hsla(0,0%,100%,.65)",
        "nav-link-color": "rgba(255,255,255,0.87)"
    }
}