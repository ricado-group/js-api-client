{
  "name": "@ricado/api-client",
  "version": "0.1.49",
  "description": "RICADO Gen 4 API Client Library for NodeJS and Browsers",
  "author": {
    "name": "Ash Neilson"
  },
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ricado-group/js-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/ricado-group/js-api-client/issues"
  },
  "license": "MIT",
  "keywords": [
    "ricado",
    "gen4",
    "api",
    "client"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf dist_browser",
    "lint": "eslint",
    "build": "genversion --semi --es6 src/PackageVersion.js && tsc && cross-env babel src --out-dir dist",
    "build:browser": "npm run build && cross-env NODE_ENV=production webpack -p --config webpack.config.js",
    "build:watch": "./node_modules/.bin/watch \"npm run build\" src",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.conf.json",
    "prepublishOnly": "npm run clean && npm run build:browser && npm run docs",
    "types-lint": "tsc && dtslint --localTs node_modules/typescript/lib types"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@microsoft/rush-stack-compiler-3.5": "0.4.3",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.2.1",
    "dts-bundle": "^0.7.3",
    "dtslint": "^3.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.22.0",
    "genversion": "^2.2.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "jsdoc-memberof-namespace": "^2.2.0",
    "rimraf": "^2.7.1",
    "typescript": "^3.9.6",
    "watch": "^1.0.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  }
}
